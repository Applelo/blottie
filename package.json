{
  "name": "blottie",
  "type": "module",
  "version": "1.0.0",
  "license": "GPL-3.0",
  "description": "Lottie component for VueJS 3 / Nuxt 3 ",
  "main": "./dist/blottie.umd.cjs",
  "module": "./dist/blottie.js",
  "engines": {
    "node": ">=14"
  },
  "exports": {
    ".": {
      "import": "./dist/blottie.js",
      "require": "./dist/blottie.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "types": "dist",
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "lint": "prettier -c .",
    "lint:fix": "prettier --write --cache .",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "lottie",
    "lottie-web",
    "animation",
    "vue",
    "typed",
    "typescript",
    "ts",
    "type"
  ],
  "author": "Applelo",
  "dependencies": {
    "lottie-web": "^5.10.2",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@types/lodash.get": "^4.4.7",
    "@types/node": "^18.13.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "lodash.get": "^4.4.2",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vue-tsc": "^1.0.24"
  },
  "publishConfig": {
    "access": "public"
  }
}
